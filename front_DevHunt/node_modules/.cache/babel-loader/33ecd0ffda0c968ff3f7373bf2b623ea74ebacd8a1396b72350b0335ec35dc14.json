{"ast":null,"code":"import { Clubs } from \"@/data\";\n// this.$route.params.id\nexport default {\n  data() {\n    return {\n      isToggled: false,\n      Clubs,\n      index: this.$route.params.id,\n      list: [1, 2, 3, 4, 5]\n    };\n  },\n  methods: {\n    changeIndex(ind) {\n      this.index = ind;\n    },\n    toggle1(id) {\n      this.$refs.inner[id - 1].classList.toggle(\"is_flipped\");\n    }\n  }\n};","map":{"version":3,"names":["Clubs","data","isToggled","index","$route","params","id","list","methods","changeIndex","ind","toggle1","$refs","inner","classList","toggle"],"sources":["src/views/Services.vue"],"sourcesContent":["<template>\n  <section class=\"pt-5\">\n    <div class=\"container shadow section-sm rounded\">\n      <div class=\"row\">\n        <div class=\"col-lg-3\">\n          <ul class=\"sidenav\" v-for=\"(data, id) in Clubs\" :key=\"id\">\n            <li class=\"sidelist c-type\" @click=\"changeIndex(id)\">\n              <span :class=\"id == index && 'isActive'\">{{ data.title }}</span>\n            </li>\n          </ul>\n        </div>\n        <div class=\"col-lg-8\">\n          <div class=\"px-lg-5 px-4\">\n            <h2 class=\"mb-4 font-weight-medium\">\n              {{ Clubs[index].title }}\n            </h2>\n            <div class=\"content\">\n              <div class=\"row\">\n                <div\n                  class=\"card__inner\"\n                  v-for=\"item in list\"\n                  :key=\"item\"\n                  @click=\"toggle1(item)\"\n                  :class=\"isToggled ? 'is_flipped', '' \"\n                >\n                  <div class=\"card__face card__face--front\">\n                    <div class=\"tojo\">\n                      <img\n                        :src=\"Clubs[index].image\"\n                        width=\"500px\"\n                        alt=\"image\"\n                      />\n                      <div class=\"fita\"><p>zryreyeqrquuer</p></div>\n                    </div>\n                  </div>\n                  <div class=\"card__face card__face--back\">\n                    <div class=\"card__content\">\n                      <div class=\"card__header\">\n                        <h2>Biographie</h2>\n                      </div>\n                      <div class=\"card__body\">\n                        <h3>JavaScript Wizard</h3>\n                        <p>\n                          Lorem ipsum <strong>dolor</strong> sit amet,\n                          consectetur <strong>adipiscing</strong> elit. Sed id\n                          erat a magna lobortis dictum. Nunc est arcu,\n                          <strong>lacinia</strong> quis sapien placerat,\n                          <strong>laoreet</strong> tincidunt nulla.\n                        </p>\n                      </div>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <!-- </div> -->\n  </section>\n</template>\n<script>\nimport { Clubs } from \"@/data\";\n// this.$route.params.id\nexport default {\n  data() {\n    return {\n      isToggled : false ,\n      Clubs,\n      index: this.$route.params.id,\n      list: [1, 2, 3, 4, 5],\n    };\n  },\n  methods: {\n    changeIndex(ind) {\n      this.index = ind;\n    },\n    toggle1(id) {\n      this.$refs.inner[id - 1].classList.toggle(\"is_flipped\");\n    },\n  },\n};\n</script>\n<style scoped>\nspan {\n  padding: 20px 0;\n  color: #222;\n  display: block;\n  font-size: 18px;\n  font-weight: 500;\n}\nspan:hover {\n  color: var(--baseColor) !important;\n  position: relative;\n  margin-left: 10px;\n}\n.isActive {\n  color: var(--baseColor) !important;\n  position: relative;\n  margin-left: 10px;\n}\n.isActive::before {\n  position: absolute;\n  content: \"\";\n  left: -10px;\n  top: 25%;\n  height: 50%;\n  width: 1px;\n  background: var(--baseColor);\n}\n/*rsytududÂµ*/\n\n.tojo {\n  position: relative;\n}\n.fita {\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n}\n\n.card__inner {\n  font-family: montserrat, sans-serif;\n  width: 300px;\n  height: 500px;\n  perspective: 1000px;\n  transition: transform 1s;\n  transform-style: preserve-3d;\n  margin-left: 11%;\n  margin-top: 2%;\n  margin-bottom: 2%;\n  position: relative;\n}\n\n.card__inner.is_flipped {\n  transform: rotateY(180deg);\n}\n\n.card__face {\n  position: absolute;\n  width: 100%;\n  height: 100%;\n  -webkit-backface-visibility: hidden;\n  backface-visibility: hidden;\n  overflow: hidden;\n  border-radius: 16px;\n  box-shadow: 0px 3px 18px 3px rgba(0, 0, 0, 0.2);\n}\n\n.card__face--front {\n  background-image: linear-gradient(to bottom right, #00d362, #ffff);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.card__face--front h2 {\n  color: #fff;\n  font-size: 32px;\n}\n\n.card__face--back {\n  background-color: #f3f3f3;\n  transform: rotateY(180deg);\n}\n\n.card__content {\n  width: 100%;\n  height: 100%;\n}\n\n.card__header {\n  position: relative;\n  padding: 30px 30px 40px;\n}\n\n.card__header:after {\n  content: \"\";\n  display: block;\n  position: absolute;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  background-image: linear-gradient(to bottom left, #00d362 10%, #ffff 115%);\n  z-index: -1;\n  border-radius: 0px 0px 50% 0px;\n}\n\n.pp {\n  display: block;\n  width: 128px;\n  height: 128px;\n  margin: 0 auto 30px;\n  border-radius: 50%;\n  background-color: #fff;\n  border: 5px solid #fff;\n  object-fit: cover;\n}\n\n.card__header h2 {\n  color: #fff;\n  font-size: 32px;\n  font-weight: 900;\n  text-transform: uppercase;\n  text-align: center;\n}\n\n.card__body {\n  padding: 30px;\n}\n\n.card__body h3 {\n  color: #212121;\n  font-size: 24px;\n  font-weight: 600;\n  margin-bottom: 15px;\n}\n\n.card__body p {\n  color: #212121;\n  font-size: 18px;\n  line-height: 1.4;\n}\n</style>\n\n"],"mappings":"AA+DA,SAAAA,KAAA;AACA;AACA;E