{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { backServer } from '@/config/axiosConfig';\nexport default {\n  data() {\n    return {\n      user: {},\n      isValid: {\n        username: false,\n        pass: false\n      },\n      isDisable: true\n    };\n  },\n  methods: {\n    logIn() {\n      backServer.post('/log', this.user).then(res => {\n        if (res.data.message === \"connexion etablie\") {\n          localStorage.setItem(\"token\", res.data.access_token);\n          localStorage.setItem(\"name\", res.data.info_user.name);\n          localStorage.setItem(\"id\", res.data.info_user.id);\n          if (res.data.info_user.role === \"admin\") {\n            localStorage.setItem(\"role\", res.data.info_user.role);\n            this.$router.push('/admin/entrer');\n          } else {\n            this.$router.push('/entrer');\n          }\n        } else {\n          this.errorMsg = res.data.message;\n          this.toggleError = true;\n          setTimeout(this.clearMessage, 5000);\n          this.$router.push('/');\n        }\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n    login() {\n      backServer.post('/login', this.user).then(res);\n      this.$router.push(`/Chat`);\n    },\n    inputHandler(e) {\n      switch (e.target.id) {\n        case 'username':\n          e.target.value.length > 0 ? this.isValid['username'] = true : this.isValid['username'] = false;\n          break;\n        case 'password':\n          e.target.value.length >= 6 ? this.isValid['pass'] = true : this.isValid['pass'] = false;\n          break;\n      }\n      if (this.isValid['username'] && this.isValid['pass']) {\n        this.isDisable = false;\n      } else {\n        this.isDisable = true;\n      }\n    }\n  }\n};","map":{"version":3,"names":["backServer","data","user","isValid","username","pass","isDisable","methods","logIn","post","then","res","message","localStorage","setItem","access_token","info_user","name","id","role","$router","push","errorMsg","toggleError","setTimeout","clearMessage","catch","err","console","log","login","inputHandler","e","target","value","length"],"sources":["src/components/SignIn.vue"],"sourcesContent":["<template>\n  <div class=\"row\">\n          <div class=\"col-lg-3\">\n          <div  class=\"avatar-container d-smart-none bg-light\">\n             <label for=\"formFileLg\">\n                <img alt=\"Avatar\" class=\"avatar-image\" >\n              </label>\n            <div class=\"avatar-middle\">\n                Photo\n            </div>\n          </div>\n          </div>\n          <div class=\"col-lg-8\">\n          <div class=\"px-lg-5 px-4\">\n            <h2 class=\"mb-4 font-weight-medium\">LogIn</h2>\n            <div class=\"content\">\n                <form class=\" sign-up\" @submit.prevent=\"login\">\n                  <div class=\"form-floating mb-3\">\n                    <input type=\"text\" class=\"form-control\" :class=\"isValid['username'] ? 'is-valid' : 'not-valid'\" id=\"username\" placeholder=\"\" autocomplete=\"off\" required v-model=\"user['username']\" @keyup=\"inputHandler\">\n                    <label for=\"username\">Username or Email</label>\n                  </div>\n                  <div class=\"form-floating mb-3\">\n                    <input type=\"password\" class=\"form-control\" :class=\"isValid['pass'] ? 'is-valid' : 'not-valid'\" id=\"password\" placeholder=\"\" autocomplete=\"off\" required v-model=\"user['password']\" @keyup=\"inputHandler\">\n                    <label for=\"password\">Password</label>\n                  </div>\n                  <div class=\"d-flex align-items-center justify-content-end\">\n                    <button type=\"submit\" class=\"btn btn-primary\" :disabled=\"isDisable\">Login</button>\n                  </div>\n                </form>\n            </div>\n          </div>\n          </div>\n        </div>\n</template>\n<script>\n\n  import { backServer } from '@/config/axiosConfig'\n\n  export default{\n\n    data(){\n      return{\n          user:{},\n          isValid:{username: false, pass: false},\n          isDisable: true,\n      }\n    },\n    methods: {\n      logIn(){ \n        backServer.post('/log',this.user)\n          .then(res => {\n              if(res.data.message === \"connexion etablie\"){\n                localStorage.setItem(\"token\",res.data.access_token )\n                localStorage.setItem(\"name\",res.data.info_user.name)\n                localStorage.setItem(\"id\",res.data.info_user.id)\n                if(res.data.info_user.role === \"admin\"){\n                  localStorage.setItem(\"role\",res.data.info_user.role)\n                  this.$router.push('/admin/entrer')  \n                }else {\n                  this.$router.push('/entrer')\n                }\n              }else {\n                this.errorMsg = res.data.message\n                this.toggleError = true\n                setTimeout(this.clearMessage,5000)\n                this.$router.push('/')\n              }\n          }).catch(err => {\n              console.log(err);\n          })\n        },\n      login(){\n        \n        backServer.post('/login',this.user)\n          .then(res)\n        this.$router.push(`/Chat`)  \n      },\n      inputHandler(e){\n        switch (e.target.id) {\n          case 'username':\n              e.target.value.length >0 ? this.isValid['username'] = true : this.isValid['username'] = false\n            break;\n          case 'password':\n              e.target.value.length >=6 ? this.isValid['pass'] = true : this.isValid['pass'] = false\n            break;\n        }\n\n        if(this.isValid['username'] && this.isValid['pass'])\n        {\n          this.isDisable = false\n        }else{\n          this.isDisable = true\n        }\n      },\n      \n    }\n  }\n</script>"],"mappings":";AAoCA,SAAAA,UAAA;AAEA;EAEAC,KAAA;IACA;MACAC,IAAA;MACAC,OAAA;QAAAC,QAAA;QAAAC,IAAA;MAAA;MACAC,SAAA;IACA;EACA;EACAC