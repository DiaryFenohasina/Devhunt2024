tname':\n              e.target.value.length >0 ? this.isValid['f_name'] = true : this.isValid['f_name'] = false\n            break;\n          case 'lastname':\n              e.target.value.length >0 ? this.isValid['l_name'] = true : this.isValid['l_name'] = false\n            break;\n          case 'email':\n              /^[a-z0-9._-]+@[a-z0-9._-]+\\.[a-z]{2,6}$/.test(e.target.value) ? this.isValid['email'] = true : this.isValid['email'] = false\n            break;\n          case 'password':\n              e.target.value.length >=6 ? this.isValid['pass'] = true : this.isValid['pass'] = false\n            break;\n          case 'c_password':\n              (e.target.value.length >=6 && e.target.value == this.user['password']) ? this.isValid['c_pass'] = true : this.isValid['c_pass'] = false\n            break;\n        }\n        \n        if(this.isValid['f_name'] && this.isValid['l_name'] && this.isValid['email'] && this.isValid['pass'] && this.isValid['c_pass'])\n        {\n          this.isDisable = false\n        }else{\n          this.isDisable = true\n        }\n        \n      }\n    }\n  }\n</script>\n<style scoped>\nspan {\n  padding: 20px 0;\n  color: #222;\n  display: block;\n  font-size: 18px;\n  font-weight: 500;\n}\nspan:hover {\n  color: var(--baseColor) !important;\n  position: relative;\n  margin-left: 10px;\n}\n.isActive {\n  color: var(--baseColor) !important;\n  position: relative;\n  margin-left: 10px;\n}\n.isActive::before {\n  position: absolute;\n  content: \"\";\n  left: -10px;\n  top: 25%;\n  height: 50%;\n  width: 1px;\n  background: var(--baseColor);\n}\n.sign-up .form-floating .form-control {\n  font-size: 16px;\n  border: 0;\n  outline: 0;\n}\n.sign-up .form-control.active.focus-visible {\n  border-bottom-left-radius: 0;\n  border-bottom-right-radius: 0;\n}\n.sign-up button i {\n  color: var(--baseColor);\n}\n.sign-up .form-control{\n  /* box-shadow: 0 4px 6px 0 rgba(62, 62, 62, 0.102), 0 6px 60px 0 rgba(62, 62, 62, 0.102) !important