{"ast":null,"code":"import avatar from '@/assets/images/avatar.png';\nexport default {\n  data() {\n    return {\n      user: {},\n      avatar,\n      isValid: {\n        f_name: false,\n        l_name: false,\n        email: false,\n        pass: false,\n        c_pass: false,\n        file: false\n      },\n      isDisable: true,\n      sign: this.$route.path\n    };\n  },\n  methods: {\n    fileHandler(e) {\n      console.log(e.target.files);\n      let files = e.target.files || e.dataTransfer.files;\n      if (!files.length) return;\n      this.createImg(files[0]);\n    },\n    createImg(file) {\n      let reader = new FileReader();\n      reader.onload = e => {\n        this.avatar = e.target.result;\n      };\n      reader.readAsDataURL(file);\n    },\n    inputHandler(e) {\n      switch (e.target.id) {\n        case 'firstname':\n          e.target.value.length > 0 ? this.isValid['f_name'] = true : this.isValid['f_name'] = false;\n          break;\n        case 'lastname':\n          e.target.value.length > 0 ? this.isValid['l_name'] = true : this.isValid['l_name'] = false;\n          break;\n        case 'email':\n          /^[a-z0-9._-]+@[a-z0-9._-]+\\.[a-z]{2,6}$/.test(e.target.value) ? this.isValid['email'] = true : this.isValid['email'] = false;\n          break;\n        case 'password':\n          e.target.value.length >= 6 ? this.isValid['pass'] = true : this.isValid['pass'] = false;\n          break;\n        case 'c_password':\n          e.target.value.length >= 6 && e.target.value == this.user['password'] ? this.isValid['c_pass'] = true : this.isValid['c_pass'] = false;\n          break;\n      }\n      if (this.isValid['f_name'] && this.isValid['l_name'] && this.isValid['email'] && this.isValid['pass'] && this.isValid['c_pass']) {\n        this.isDisable = false;\n      } else {\n        this.isDisable = true;\n      }\n    }\n  }\n};","map":{"version":3,"names":["avatar","data","user","isValid","f_name","l_name","email","pass","c_pass","file","i