\n                  </div>\n                  <div class=\"form-floating mb-3\">\n                    <input type=\"password\" class=\"form-control\" :class=\"isValid['pass'] ? 'is-valid' : 'not-valid'\" id=\"password\" placeholder=\"\" required v-model=\"user['password']\" @keyup=\"inputHandler\">\n                    <label for=\"password\">Password</label>\n                  </div>\n                  <div class=\"form-floating mb-3\">\n                    <input type=\"password\" class=\"form-control\" :class=\"isValid['c_pass'] ? 'is-valid' : 'not-valid'\" id=\"c_password\" placeholder=\"\" required v-model=\"user['c_password']\" @keyup=\"inputHandler\">\n                    <label for=\"c_password\">Confirm password</label>\n                  </div>\n                  <div class=\"mb-5 d-lg-none\">\n                    <label for=\"formFileLg\" class=\"form-label\">Photo</label>\n                    <input class=\"form-control form-control-lg\" id=\"formFileLg\" @change=\"fileHandler\" type=\"file\" >\n                  </div>\n                  <div class=\"d-flex align-items-center justify-content-end\">\n                    <router-link to=\"#\"><span class=\"mx-2 mx-sm-3 has-acc\">Have an account ?</span></router-link>\n                    <button type=\"submit\" class=\"btn btn-primary\" :disabled=\"isDisable\">Create Account</button>\n                  </div>\n                </form>\n            </div>\n          </div>\n          </div>\n        </div>\n    </div>\n  </section>\n</template>\n<script>\n  import avatar from '@/assets/images/avatar.png'\n  export default{\n    data(){\n      return{\n          user:{},\n          avatar,\n          isValid:{f_name: false, l_name: false, email: false, pass: false, c_pass: false, file: false},\n          isDisable: true,\n          sign: this.$route.path,\n      }\n    },\n    methods: {\n      fileHandler(e){\n        let files = e.target.files || e.dataTransfer.files\n      },\n      createImg(file){\n        let reader = new FileReader();\n        reader.onload = (e) => {\n          this.avatar = e.target.result\n        }\n        reader.readAsDataURL(file)\n      },\n      inputHandler(e){\n        switch (e.target.id) {\n          case 'firstname':\n              e.target.value.length >0 ? this.isValid['f_name'] = true : this.isValid['f_name'] = false\n            break;\n          case 'lastname':\n              e.target.value.length >0 ? this.isValid['l_name'] = true : this.isValid['l_name'] = false\n            break;\n          case 'email':\n              /^[a-z0-9._-]+@[a-z0-9._-]+\\.[a-z]{2,6}$/.test(e.target.value) ? this.isValid['email'] = true : this.isValid['email'] = false\n            break;\n          case 'password':\n              e.target.value.length >=6 ? this.isValid['pass'] = true : this.isValid['pass'] = false\n            break;\n          case 'c_password':\n              (e.target.value.length >=6 && e.target.value == this.user['password']) ? this.isValid['c_pass'] = true : this.isValid['c_pass'] = false\n            break;\n        }\n        \n        if(this.isValid['f_name'] == this.isValid['l_name'] == this.isValid['email'] == this.isValid['pass'] == this.isValid['c_pass'] == true )\n        {\n          this.isDisable = false\n        }else{\n          this.isDisable = true\n        }\n        \n      }\n    }\n  }\n</script>\n<style scoped>\nspan {\n  padding: 20px 0;\n  color: #222;\n  display: block;\n  font-size: 18px;\n  font-weight: 500;\n}\nspan:hover {\n  color: var(--baseColor) !important;\n  position: relative;\n  margin-left: 10px;\n}\n.isActive {\n  color: var(--baseColor) !important;\n  position: relative;\n  margin-left: 10px;\n}\n.isActive::before {\n  position: absolute;\n  content: \"\";\n  left: -10px;\n  top: 25%;\n  height: 50%;\n  width: 1px;\n  background: var(--baseColor);\n}\n.sign-up .form-floating .form-control {\n  font-size: 16px;\n  border: 0;\n  outline: 0;\n}\n.sign-up .form-control.active.focus-visibl